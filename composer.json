{
    "name": "assoconnect/php-percent",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/php-percent/"
        }
    ],
    "type": "library",
    "description": "PHP object to handle percentage with MoneyPHP",
    "keywords": ["php", "percent"],
    "homepage": "https://github.com/assoconnect/php-percent/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PHPPercent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PHPPercent\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3"
    },
    "require": {
        "php": "^7.4",
        "moneyphp/money": "^3.3"
    }
}
